{
    "projectFolder": "../",
    "mainEntryPointFilePath": "<projectFolder>/dist/build/index.d.ts",
    "bundledPackages": [],
    "compiler": {
      "tsconfigFilePath": "<projectFolder>/config/tsconfig.json",
      "overrideTsconfig": {
        "compilerOptions": {
          "outDir": "build"
        }
      }
    },
    "dtsRollup": {
      "enabled": true,
      "untrimmedFilePath": "<projectFolder>/dist/build/index.d.ts"
    },
    "apiReport": {
      "enabled": false
    },
    "docModel": {
      "enabled": false
    },
    "tsdocMetadata": {
      "enabled": false
    },
    "messages": {
      "compilerMessageReporting": {
        "default": {
          "logLevel": "none"
        }
      },
      "extractorMessageReporting": {
        "default": {
          "logLevel": "none"
        }
      },
      "tsdocMessageReporting": {
        "default": {
          "logLevel": "none"
        }
      }
    }
  }