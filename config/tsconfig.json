{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": "../",
    "target": "ES2015",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "strictPropertyInitialization": false
  },
  "include": ["../src/**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts"]
}
